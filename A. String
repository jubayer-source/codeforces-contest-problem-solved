//A. String
//https://codeforces.com/contest/2062/problem/A

#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        string s;
        cin >> s;

        int n = s.size();

        int operations = 0;

        for (int i = 0; i < n; i++)
        {
            if (s[i] == '1')
            {

                operations++;

            }
        }
        cout << operations <<endl;
    }

    return 0;
}
